{
    "name": "buyer-show",
    "version": "0.1.0",
    "description": "buyer show",
    "author": "",
    "private": true,
    "scripts": {
        "watchify": "watchify -vd -p browserify-hmr --global-transform babelify --global-transform vueify --global-transform [ browserify-css --minify=true ] -e src/main.js -o dist/build.js",
        "serve": "http-server -c 1 -a localhost",
        "dev": "npm-run-all --parallel watchify serve",
        "build": "cross-env NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o dist/build.css ] --global-transform babelify --global-transform vueify --global-transform [ browserify-css --minify=true ]  -e src/main.js | uglifyjs -c warnings=false -m > dist/build.js",
        "push": "git push origin master"
    },
    "browserify": {
        "transform": []
    },
    "dependencies": {
        "vue": "^2.0.1"
    },
    "devDependencies": {
        "babel-core": "^6.0.0",
        "babel-plugin-transform-runtime": "^6.0.0",
        "babel-polyfill": "^6.23.0",
        "babel-preset-es2015": "^6.0.0",
        "babel-preset-stage-2": "^6.0.0",
        "babel-runtime": "^6.0.0",
        "babelify": "^7.2.0",
        "browserify": "^13.1.0",
        "browserify-css": "^0.10.1",
        "browserify-hmr": "^0.3.1",
        "cross-env": "^2.0.0",
        "element-ui": "^1.3.7",
        "envify": "^3.4.1",
        "http-server": "^0.9.0",
        "npm-run-all": "^2.3.0",
        "proxyquireify": "^3.0.1",
        "superagent": "^3.5.2",
        "uglify-js": "^2.5.0",
        "vueify": "^9.0.0",
        "watchify": "^3.4.0"
    }
}